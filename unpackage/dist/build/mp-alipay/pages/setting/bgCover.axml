<view class="container"><view class="panel"><view class="panel-title"><text>默认皮肤</text></view><view class="cell"><block a:for="{{bgCoverlist}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['selectbgCover',['$0',index],[[['bgCoverlist','',index]]]]]]]}}" class="{{(('coin-box')+' '+((bgCoverIndex===index)?'selectActive':''))}}" onTap="__e"><image class="bg-img" src="{{item.src}}" mode="aspectFill"></image><view class="bg-text"><view>{{item.name}}</view><block a:if="{{item.id===bgCovername}}"><view class="coin-spec">使用中</view></block></view></view></block></view></view><block a:if="{{bgCoverIndex!==-1}}"><view class="footer"><button class="footer-btn" style="background:#fd746c;" type="primary" data-event-opts="{{[['tap',[['setbgCover']]]]}}" onTap="__e">立即使用</button></view></block></view>